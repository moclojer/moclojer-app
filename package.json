{
  "name": "front-helix",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npx shadow-cljs compile app",
    "ci:karma-make": "npx msw init resources/front/test",
    "ci:tests": "npm run ci:karma-make && npx shadow-cljs compile ci-tests && npx karma start --single-run",
    "app:watch": "npx shadow-cljs watch tests app",
    "shadow:release": "npx shadow-cljs release app",
    "postcss:build": "npx cross-env TAILWIND_MODE=build postcss src/front/css/tailwind.css -o ./resources/front/public/assets/css/output.css --verbose",
    "postcss:watch": "npx cross-env TAILWIND_MODE=watch postcss src/front/css/tailwind.css -o ./resources/front/public/assets/css/output.css --verbose -w",
    "postcss:release": "npx cross-env NODE_ENV=production postcss src/front/css/tailwind.css -o ./resources/front/public/assets/css/output.css --verbose",
    "watch": "run-p -l *:watch",
    "release": "run-s *:release",
    "repl": "npx shadow-cljs clj-repl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moclojer/front-helix.git"
  },
  "bugs": {
    "url": "https://github.com/moclojer/front-helix/issues"
  },
  "homepage": "https://github.com/moclojer/front-helix",
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cljs-test": "^0.1.0",
    "msw": "^0.47.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-cli": "^10.1.0",
    "shadow-cljs": "^2.23.3",
    "tailwindcss": "^3.3.2"
  },
  "dependencies": {
    "@codemirror/autocomplete": "0.18.8",
    "@codemirror/closebrackets": "0.19.2",
    "@codemirror/commands": "0.18.3",
    "@codemirror/comment": "0.18.1",
    "@codemirror/fold": "0.18.2",
    "@codemirror/gutter": "0.18.4",
    "@codemirror/highlight": "0.18.4",
    "@codemirror/history": "0.18.1",
    "@codemirror/language": "0.18.2",
    "@codemirror/lint": "0.18.6",
    "@codemirror/matchbrackets": "0.18.0",
    "@codemirror/rectangular-selection": "0.18.1",
    "@codemirror/search": "0.18.4",
    "@codemirror/state": "0.18.7",
    "@codemirror/view": "0.18.19",
    "@supabase/supabase-js": "^2.25.0",
    "@tailwindcss/forms": "^0.5.3",
    "cross-env": "^7.0.3",
    "js-cookie": "^3.0.5",
    "postcss-focus-visible": "^8.0.2",
    "puppeteer": "^19.9.1",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.13.0",
    "use-sync-external-store": "1.2.0",
    "flowbite": "^1.6.6",
    "flowbite-react": "^0.4.7",
    "svgmap": "^2.10.1"
  },
  "msw": {
    "workerDirectory": "resources/front/test"
  }
}
