  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-cljs-test": "^0.1.0",
    "msw": "^0.47.4",
    "npm-run-all": "^4.1.5",
    "shadow-cljs": "^2.20.7",
    "postcss": "^8.4.14",
